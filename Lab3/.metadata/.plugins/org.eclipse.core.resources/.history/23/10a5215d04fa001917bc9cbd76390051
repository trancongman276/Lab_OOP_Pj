
public class Student {
	private String name,ID;
	private int Score;
	public Student() {
		
	}
	
	public int getScore() {
		return Score;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void setID(String iD) {
		ID = iD;
	}

	public void setScore(int score) {
		Score = score;
	}
	
}
